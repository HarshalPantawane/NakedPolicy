{
    "manifest_version": 3,
    "name": "NakedPolicy",
    "version": "1.0",
    "description": "Understand website privacy risks instantly",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "http://localhost:5000/*"
    ]
}